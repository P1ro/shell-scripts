$ function diff-rev () {
$     local diff_program="diff"
$     if [ "$1" = "--diff-prog" ]; then
$         diff_program="$2"
$         shift 2
$     fi
$     local right_file="$1"
$     local left_file="$2"
$     if [ -d "$left_file" ]; then left_file="$left_file"/$(basename "$right_file"); fi
$     "$diff_program" "$left_file" "$right_file"
$ }
